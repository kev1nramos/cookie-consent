{
  "name": "@kev1nramos/cookie-consent-core",
  "version": "1.0.0",
  "description": "Framework-agnostic cookie consent management library with GDPR compliance",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "cookie",
    "consent",
    "gdpr",
    "ccpa",
    "privacy",
    "typescript"
  ],
  "author": "Kevin Ramos <kevin@kev1nramos.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kev1nramos/cookie-consent",
    "directory": "packages/core"
  },
  "packageManager": "pnpm@10.20.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "tsup": "^8.5.0"
  }
}
